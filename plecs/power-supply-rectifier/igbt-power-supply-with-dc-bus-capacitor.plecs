              Parameter {
                Variable      "Declarations"
                Value         "#import <math.h>\n"
"#import <complex.h>\n"
"\n"
"#define i_d_ref InputSignal(0,0)\n"
"#define i_q_ref InputSignal(0,1)\n"
"#define i_d InputSignal(1,0)\n"
"#define i_q InputSignal(1,1)\n"
"#define theta_g InputSignal(2,0)\n"
"#define u_dc InputSignal(3,0)\n"
"\n"
"//System parameters\n"
"#define alpha_c ParamRealData(0,0)\n"
"#define L_f ParamRealData(1,0)\n"
"#define w_g ParamRealData(2,0)\n"
"#define u_gN ParamRealData(3,0)\n"
"#define T_s ParamRealData(4,0)\n"
"\n"
"#define PI 3.14159265358979323846\n"
"\n"
"static float complex u_i;\n"
"\n"
"float complex i_c_ref, u_ref, u_s_ref, u_s_ref_lim, u_ref_lim, u_i_new, i_c, "
"k_p;\n"
"float theta_gc, u_alpha_ref, u_beta_ref, u_a_ref, u_b_ref, u_c_ref, max_u_val"
"ue, min_u_value, u0, m, k_i, k_t;\n"
"float u_abc_ref[3], u_abc_ref_for_m[4], d_abc[3];"
                Show          off
              }
              Parameter {
                Variable      "StartFcn"
                Value         "//Initial condition\n"
"u_i = u_gN + I*0;"
                Show          off
              }
              Parameter {
                Variable      "OutputFcn"
                Value         "// Gains\n"
"k_i = pow(alpha_c, 2)*L_f;      // Integral gain\n"
"k_t = alpha_c*L_f;              // Reference gain\n"
"k_p = ((2*alpha_c) - (I*w_g))*L_f;  // Proportional gain\n"
"\n"
"// Calculating i from i_d and i_q\n"
"i_c = i_d + I*i_q;\n"
"\n"
"// Calculating i_ref from i_d_ref and i_q_ref\n"
"i_c_ref = i_d_ref + I*i_q_ref;\n"
"\n"
"// Voltage reference in synchronous coordinates\n"
"u_ref = (k_t*i_c_ref) - (k_p*i_c) + u_i;\n"
"\n"
"// Reference in stator coordinates\n"
"theta_gc = theta_g + 1.5*T_s*w_g;   // Compensation for discrete-time delays"
"\n"
"//u_s_ref = u_ref*cexp(I*theta_gc);\n"
"\n"
"u_s_ref = (crealf(u_ref)*cos(theta_gc) - cimagf(u_ref)*sin(theta_gc)) + I*(ci"
"magf(u_ref)*cos(theta_gc) + crealf(u_ref)*sin(theta_gc));\n"
"\n"
"// Phase-voltage references\n"
"u_abc_ref[0] = crealf(u_s_ref);\n"
"u_abc_ref[1] = ((-(crealf(u_s_ref))) + (sqrt(3)*cimagf(u_s_ref)))/2;\n"
"u_abc_ref[2] = ((-(crealf(u_s_ref))) - (sqrt(3)*cimagf(u_s_ref)))/2;\n"
"\n"
"// Finding maximum value\n"
"max_u_value = u_abc_ref[0];\n"
"for(int index = 0; index < 3; index++) {\n"
"\tif(u_abc_ref[index] > max_u_value){\n"
"      max_u_value = u_abc_ref[index];\n"
"  \t}\n"
"}\n"
"\n"
"//Finding minimum value\n"
"min_u_value = u_abc_ref[0];\n"
"for(int index = 0; index < 3; index++) {\n"
"\tif(u_abc_ref[index] < min_u_value){\n"
"      min_u_value = u_abc_ref[index];\n"
"  \t}\n"
"}\n"
"\n"
"// Symmetrization of the references by adding the zero-sequence voltage\n"
"u0 = (max_u_value + min_u_value)/2;\n"
"u_abc_ref[0] = u_abc_ref[0] - u0;\n"
"u_abc_ref[1] = u_abc_ref[1] - u0;\n"
"u_abc_ref[2] = u_abc_ref[2] - u0;\n"
"\n"
"// Clipping of the reference voltages such that no phase error is caused. \n"
"// If all phase voltage reference are realizable (below u_dc/2), m = 1 holds."
"\n"
"m = (2*u_abc_ref[0])/u_dc;\n"
"for(int index = 0; index < 3; index++) {\n"
"\tfloat m_temp = (2*u_abc_ref[index])/u_dc;\n"
"\tif(m_temp > m){\n"
"      m = m_temp;\n"
"  \t}\n"
"}\n"
"if(m < (float) 1){\n"
"\tm = (float) 1;\n"
"}\n"
"\n"
"//Clipping of reference voltage\n"
"u_abc_ref[0] = u_abc_ref[0]/m;\n"
"u_abc_ref[1] = u_abc_ref[1]/m;\n"
"u_abc_ref[2] = u_abc_ref[2]/m;\n"
"\n"
"\n"
"// Duty cycles, which can be directly used for triangle comparison, \n"
"// if the triangle wave varies between 0...1\n"
"d_abc[0] = 0.5 + (u_abc_ref[0]/u_dc);\n"
"d_abc[1] = 0.5 + (u_abc_ref[1]/u_dc);\n"
"d_abc[2] = 0.5 + (u_abc_ref[2]/u_dc);\n"
"\n"
"\n"
"// Realized voltage vector is calculated from the clamped duty ratios.\n"
"// If the PWM does not saturate, u_c_ref_lim = u_c_ref holds. The realized\n"
"// voltage vector is needed, e.g., for the current-controller antiwindup.\n"
"u_s_ref_lim = (2*u_dc*(d_abc[0] + d_abc[1]*cexp(I*(2.*PI/3)) + d_abc[2]*cexp("
"I*(4.*PI/3))))/3;\n"
"\n"
"// In estimated rotor flux coordinates\n"
"u_ref_lim = u_s_ref_lim*cexp(-I*theta_gc);\n"
"\n"
"u_ref_lim = (crealf(u_s_ref_lim)*cos(theta_gc) + cimagf(u_s_ref_lim)*sin(thet"
"a_gc)) + I*(cimagf(u_s_ref_lim)*cos(theta_gc) - crealf(u_s_ref_lim)*sin(theta"
"_gc));\n"
"\n"
"// Update the integral state of the current controller \n"
"u_i_new = u_i + T_s*k_i*(i_c_ref - i_c + ((u_ref_lim - u_ref)/k_t));\n"
"\n"
"OutputSignal(0,0) = crealf(u_ref);\n"
"OutputSignal(0,1) = cimagf(u_ref);\n"
"\n"
"OutputSignal(1,0) = d_abc[0];\n"
"OutputSignal(1,1) = d_abc[1];\n"
"OutputSignal(1,2) = d_abc[2];"
                Show          off
              }
