"#include <complex.h>\n"
"\n"
"#define u_g_a InputSignal(0,0)\n"
"#define u_g_b InputSignal(0,1)\n"
"#define u_g_c InputSignal(0,2)\n"
"\n"
"#define PI 3.14159265358979323846\n"
"\n"
"float complex u_gs, u_g;\n"
"static float theta_g, zeta, w0_pll, T_s, w_gi;\n"
"float u_gq, w_g, w_gi_new, theta_g_new, u_gmod, k_p, k_i, u_gN;"
            Show          off
          }
          Parameter {
            Variable      "StartFcn"
            Value         "zeta = ParamRealData(0,0);\n"
"w0_pll = ParamRealData(1,0);\n"
"T_s = ParamRealData(2,0);\n"
"u_gN = ParamRealData(3,0);\n"
"\n"
"theta_g = 0;\n"
"w_gi = 2*PI*50;\n"
"\n"
"u_gs = 0;\n"
"u_g = 0;\n"
"\n"
"u_gq = 0; \n"
"w_g = 0; \n"
"w_gi_new = 0; \n"
"theta_g_new = 0;\n"
"\n"
"k_p = ((2*zeta*w0_pll)/u_gN)*pow(10, 0);\n"
"k_i = (pow(w0_pll, 2)/u_gN)*pow(10, 0);\n"
            Show          off
          }
          Parameter {
            Variable      "OutputFcn"
            Value         "u_gs = 2*(u_g_a + u_g_b*cexp(I*(2.*PI/3)) + u_g_c*c"
"exp(I*(4.*PI/3)))/3;\n"
"\n"
"u_g = u_gs*cexp(-I*theta_g);\n"
"\n"
"u_gmod = sqrt(pow(creal(u_g), 2) + pow(cimag(u_g), 2));\n"
"\n"
"u_gq = cimag(u_g);\n"
"\n"
"w_g = (k_p*u_gq) + w_gi;\n"
"\n"
"w_gi_new = w_gi + (T_s*k_i*u_gq);\n"
"\n"
"theta_g_new = theta_g + (T_s*w_g);\n"
"\n"
"if(theta_g_new > PI){\n"
"\ttheta_g_new = theta_g_new - 2*PI;\n"
"}else if(theta_g_new < -PI){\n"
"\ttheta_g_new = theta_g_new + 2*PI;\n"
"}\n"
"\n"
"OutputSignal(0,0) = theta_g_new;\n"
"OutputSignal(0,1) = creal(u_gs);\n"
"OutputSignal(0,2) = cimag(u_gs);"
            Show          off
          }
          Parameter {
            Variable      "UpdateFcn"
            Value         "w_gi = w_gi_new;\n"
"theta_g = theta_g_new;"
            Show          off
          }
